
if $custom_maprotation_enabled [
    if $custom_maprotation_balance [privmsg $cn (err "Next map is undetermined.")] [
            local ms (mapsetname)
            if (symbol? $ms) [
                privmsg $cn (info (format "Next map is %1." (nextmap (value $ms))))
            ] [privmsg $cn (err "Map rotation is client controlled - for this game mode.")]
        ]
] [privmsg $cn (err "Map rotation is client controlled.")]
