
if $use_server_maprotation [
    if $use_balanced_maprotation [privmsg $cn (err "Next mode and map is unknown to the server.")] [
            local ms (mapsetname)
            if (symbol? $ms) [
                local nextmode $gamemode
                privmsg $cn (info (format "Next map is %1 playing %2." (nextmap (value $ms)) $nextmode))
            ] [privmsg $cn (err "Map rotation is client controlled - for this game mode.")]
        ]
] [privmsg $cn (err "Map rotation is client controlled.")]
