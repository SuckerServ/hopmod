
who = [
scores = ""
foreach (players) [
parameters cn
line = (format "%1(%2)" (player_name $cn)($cn))
scores = (concat $scores $line)
]
if (! (strcmp $scores "")) [
log (concat "WHO      " $scores)
]
if (strcmp $scores "" ) [
log "Apparently no one is connected"
]
]

masterwho = [
cm = (format "%1(%2)" (player_name $currentmaster)($currentmaster))
log (concat "MASTER    " $cm)
]

getvar = [
output = (value $arg1)
line = (format "IRC         -\={GETVAR %2}\=- is %1" $output $arg1 )
log $line
]
